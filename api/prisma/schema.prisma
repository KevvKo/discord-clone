  datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user {
  id                Int           @default(autoincrement()) @id
  name              String?       @unique
  email             String?       @unique
  password          String?
  phoneNumber       String?         
  status            Boolean
  active            Boolean
  userprofile       userprofile?
}

model userprofile {
  userId            Int?         
  id                Int           @id @default(autoincrement())
  color             String        @default("#939394")
  user              user?         @relation(fields: userId, references: id)
}