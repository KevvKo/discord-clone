  datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user {
  id                Int           @default(autoincrement()) @id
  username          String?       @unique
  email             String?       @unique
  password          String?
  status            Boolean
  active            Boolean
}
