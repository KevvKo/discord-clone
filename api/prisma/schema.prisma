// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user{

  id        Int     @default(autoincrement()) @id
  username  String? @unique
  email     String? @unique
  password  String 
}